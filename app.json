{
  "expo": {
    "name": "Ankaa Design",
    "slug": "ankaa-design",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-1024.png",
    "scheme": "ankaadesign",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/icon-1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ankaadesign.management",
      "jsEngine": "hermes",
      "requireFullScreen": true,
      "associatedDomains": [
        "applinks:ankaadesign.com.br",
        "applinks:www.ankaadesign.com.br"
      ],
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification", "fetch"]
      }
    },
    "android": {
      "package": "com.ankaadesign.management",
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-foreground.png",
        "backgroundColor": "#FFFFFF"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/app/"
            },
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/task/"
            },
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/order/"
            },
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/item/"
            },
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/service-order/"
            },
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/producao/"
            },
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/estoque/"
            },
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/recursos-humanos/"
            },
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/administracao/"
            },
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/pintura/"
            },
            {
              "scheme": "https",
              "host": "ankaadesign.com.br",
              "pathPrefix": "/financeiro/"
            },
            {
              "scheme": "https",
              "host": "www.ankaadesign.com.br",
              "pathPrefix": "/app/"
            },
            {
              "scheme": "https",
              "host": "www.ankaadesign.com.br",
              "pathPrefix": "/producao/"
            },
            {
              "scheme": "https",
              "host": "www.ankaadesign.com.br",
              "pathPrefix": "/estoque/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "platforms": ["ios", "android"],
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#000000",
          "mode": "production"
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "newArchEnabled": true,
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/f8f06f52-853f-4ab6-a783-181208687fa7"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f8f06f52-853f-4ab6-a783-181208687fa7"
      },
      "apiUrl": "https://api.ankaadesign.com.br",
      "fallbackApiUrl": "http://192.168.0.16:3030"
    }
  }
}
